generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  email     String   @unique
  passw     String
  createdAt DateTime? @default(now())
}

model Commit {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  message   String
  createdAt DateTime @default(now())
}

model Visita {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  dataHora  DateTime @default(now())
}
